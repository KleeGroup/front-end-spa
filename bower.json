{
  "name": "etfeedbackapp",
  "version": "0.0.1",
  "main": "public/javascripts/app.js",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components"
  ],
  "dependencies": {
    "backbone": "~1.1.0",
    "underscore": "~1.5.2",
    "bootstrap": "~3.1.0",
    "console-polyfill": "~0.1.0",
    "jquery": "~2.1.0",
    "moment": "~2.5.1",
    "bootstrap-switch": "*",
    "i18next": "~1.7.1",
    "Faker": "~0.5.11",
    "backbone-validation": "~0.9.1",
    "backbone.marionette": "~1.6.1",
    "font-awesome": "~4.0.3",
    "bluebird": "~1.0.4",
    "loggerz": "~0.0.1"
  },
  "overrides": {
    "backbone": {
      "dependencies": {
        "underscore": "*",
        "jquery": "*"
      },
      "main": "backbone.js"
    },
    "bootstrap": {
      "dependencies": {
        "jquery": "*"
      },
      "main": [
        "dist/js/bootstrap.js",
        "dist/css/bootstrap.css",
        "dist/fonts/*"
      ]
    },
    "backbone.marionette": {
      "dependencies": {
        "backbone": "*"
      }
    },
    "backbone-validation": {
      "dependencies": {
        "backbone": "*"
      },
      "main": "src/backbone-validation.js"
    },
    "bootstrap-switch": {
      "dependencies": {
        "jquery": "*"
      },
      "main": [
        "static/js/bootstrap-switch.js",
        "static/stylesheets/bootstrap-switch.css"
      ]
    },
    "underscore": {
      "main": "underscore.js"
    },
    "i18next": {
      "main": "i18next.js"
    },
    "Faker": {
      "main": "Faker.js"
    },
    "font-awesome": {
      "main": [
        "css/font-awesome.css",
        "fonts/*"
      ]
    },
    "bluebird": {
      "main": "js/browser/bluebird.js"
    },
    "jquery": {
      "main": "dist/jquery.js"
    },
    "loggerz": {
      "main": "main/browser/logger.js"
    }
  }
}
